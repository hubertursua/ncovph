{
  "name": "ncovph-functions",
  "version": "2.1.2",
  "description": "",
  "author": "Hyubs Ursua <hyubs.u@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf ./dist && npm run build",
    "deploy": "firebase deploy --only=functions",
    "lint": "eslint . --ext .js,.ts",
    "serve": "ts-node ./src/bin/serve.ts",
    "serve:watch": "nodemon --exec npm run serve",
    "serve:emulator": "npm run build && firebase emulators:start --only functions",
    "copy:graphql": "copyfiles -u 1 \"./src/graphql/typeDefs/**/*.graphql\" ./dist",
    "copy:data": "copyfiles -u 1 \"./src/data/**/*.csv\" ./dist"
  },
  "dependencies": {
    "apollo-datasource": "^0.7.0",
    "apollo-server": "^2.11.0",
    "apollo-server-core": "^2.12.0",
    "apollo-server-express": "^2.12.0",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "firebase-admin": "^8.11.0",
    "firebase-functions": "^3.5.0",
    "glob": "^7.1.6",
    "graphql": "^14.6.0",
    "graphql-scalars": "^1.1.2",
    "moment": "^2.24.0",
    "ncovph-parser": "^1.1.13",
    "papaparse": "^5.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.29.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "firebase-functions-test": "^0.2.0",
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": "10"
  },
  "license": "ISC",
  "nodemonConfig": {
    "delay": "1000",
    "ext": "ts,graphql",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "watch": [
      "src/"
    ]
  }
}
